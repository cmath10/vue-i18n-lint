{
  "name": "vue-i18n-lint",
  "version": "0.1.0",
  "description": "Lint tool for vue-i18n package",
  "author": "zaytsev.cmath10@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "rm -fr dist && microbundle --target node -f umd,modern",
    "dev": "microbundle watch --target node",
    "lint": "eslint **/*.ts",
    "lint:fix": "eslint --fix **/*.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "vue-i18n-lint": "node ./bin/vue-i18n-lint.js"
  },
  "source": "src/index.ts",
  "bin": "bin/vue-i18n-lint.js",
  "main": "dist/vue-i18n-lint.umd.js",
  "module": "dist/vue-i18n-lint.modern.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "cac": "^6.7.3",
    "dot-object": "^2.1.4",
    "glob": "^7.1.7",
    "is-valid-glob": "^1.0.0",
    "js-yaml": "^4.1.0",
    "microbundle": "^0.13.3",
    "rollup": "^2.56.3"
  },
  "devDependencies": {
    "@types/dot-object": "^2.1.2",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.1",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^16.9.2",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
